	<menu id="root-menu" label="Openbox 3">
		<separator label="  Korora 25  "/>
		<item label="Run Command">
			<action name="Execute">
				<execute>gmrun</execute>
			</action>
		</item>
		<item label="File Manager">
			<action name="Execute">
				<execute>thunar</execute>
			</action>
		</item>
		<item label="Web Browser">
			<action name="Execute">
				<execute>palemoon</execute>
			</action>
		</item>
		<item icon="/home/lol/.icons/Super\ Flat\ Remix/apps/scalable/audacious.svg" label="Music Player">
